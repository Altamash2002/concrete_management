            <a href="/comparison/rmc_concrete" class="footer-links">RMC wise</a> <br><br>
          </div>
        </div>
        <div class="col-md-2">
          <div class="text-center text-md-start mb-3 my-md-0">
            <h3 class="footer-headings text-uppercase fw-bold mb-4">Zones</h3>
            <a href="/zone/red" class="footer-links">Red Zone</a><br><br>
            <a href="/zone/grey" class="footer-links">Grey Zone</a><br><br>
            <a href="/zone/green" class="footer-links">Green Zone</a><br><br>
          </div>
        </div>
        <!-- Grid column -->

        <!-- Grid column -->
        <div class="row col-md-3">
          <!-- Links -->
          <div class="col-2 icons-div">
            <a href="https://www.facebook.com/HouseofHiranandani/" target="_blank" class="text-white icons">
              <i class="bi bi-facebook"></i>
            </a>
          </div>
          <div class="col-2 icons-div">
            <a href="https://twitter.com/HOHExclusive/" target="_blank" class="text-white icons">
              <i class="bi bi-twitter"></i>
            </a>
          </div>
          <div class="col-2 icons-div">
            <a href="https://www.youtube.com/channel/UC5b7xMV-q5ZIkJf0SxtlRdg/" target="_blank" class="text-white icons">
              <i class="bi bi-youtube"></i>
            </a>
          </div>
          <div class="col-2 icons-div">
            <a href="https://www.instagram.com/houseofhiranandani/" target="_blank" class="text-white icons">
              <i class="bi bi-instagram"></i>
            </a>
          </div>
          <div class="col-2 icons-div">
            <a href="https://www.linkedin.com/company/house-of-hiranandani/" target="_blank" class="text-white icons">
              <i class="bi bi-linkedin"></i>
            </a>
          </div>
          <div class="col-2 icons-div">
            <a href="https://www.pinterest.com/HOHiranandani/" target="_blank" class="text-white icons">
              <i class="bi bi-pinterest"></i>
            </a>
          </div>

          <div class="col-12 d-none d-md-block text-center">
            <%= link_to root_path, class: "text-white", style: "text-decoration: none;" do %>
              <%= image_tag("hiralogo.png", alt: "Logo", height: "30px", class: "mr-2 logo-img") %>
            <% end %>
          </div>

        </div>
      </section>

      <!-- Section: Links  -->

      <!-- Copyright -->
      <div class="row container-fluid justify-content-center align-items-center pb-5 mx-0">
