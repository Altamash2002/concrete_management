<!-- edit.html.erb -->
<div class="main-container">
  <div class="scrollable-container">
    <h1 class="centered-heading text-center">Edit Concrete Strength Record</h1>
  </div>
  <div class="table-content">
    <form action="<%= concrete_path(@project) %>" method="post">
      <%= csrf_meta_tags %>
      <%= method_override(:patch) %>

      <!-- Include input fields for the attributes you want to edit -->
      <div class="form-group">
        <label for="project_name">Project Name</label>
        <input type="text" name="project[name]" id="project_name" value="<%= @project.name %>" class="form-control">
      </div>

      <!-- Add more input fields for other attributes you want to edit -->

      <button type="submit" class="btn btn-primary">Save Changes</button>
    </form>
  </div>
</div>
