                </div>
              <% end %>
            <% end %>
            <h3 class="icon-heading">BMC/TMC <br> Report </h3>
          </div>
          <div class="col-md-4 text-center justify-content-center flex-column pt-4 rounded-3 mx-auto">
              <%= link_to "/drildown_strength_testing/project" do %>
                <div class="p-0">
                  <%= image_tag("Concrete Strength Cube Record0.svg", alt: "Cement") %>
                </div>
              <% end %>
            <h3 class="icon-heading">Concrete Cube <br> Strength Record</h3>
          </div>
        </div>
      </div>

      <div class="col-md-2 mb-2 mx-2 rounded-3 bg-white">
        <div>
          <h4 class="sec-heading text-center my-2 ">Casting & Testing</h4> <hr class="my-0">
          <div class="col-md-5 py-4 w-48 text-center justify-content-evenly flex-column p-0 rounded-3 mx-auto">
            <div class="pp">
              <% if current_user.present? && current_user.ceo? || current_user.site_incharge? || current_user.project_incharge? %>
                <a href="" disabled>
                  <div class="py-0 d-flex justify-content-center align-items-center">
                    <%= image_tag("casting0.svg", alt: "Cement") %>
                  </div>
                </a>
              <% else %>
              <%= link_to "/strength_testings/new" do %>
                  <div class="py-0 d-flex justify-content-center align-items-center">
                    <%= image_tag("casting0.svg", alt: "Cement") %>
                  </div>
                <% end %>
              <% end %>
              <br>
              <h3 class="icon-heading">Daily Casting</h3> 
            </div>

          </div>     
