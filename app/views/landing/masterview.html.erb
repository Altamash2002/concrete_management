           <% if current_user.present? && current_user.lab_technician? || current_user.qa_lab_incharge?%>
              <a href="" disabled>
                <div class="py-0 d-flex justify-content-center align-items-center">
                  <%= image_tag("Overall failure0.svg", alt: "Cement") %>
                </div>
              </a>
            <% else %>
              <%= link_to "/overall_failure_report" do %>
                <div class="p-0">
                  <%= image_tag("Overall failure0.svg", alt: "Cement") %>
                </div>
              <% end %>
            <% end %>
            <h3 class="icon-heading">Over-all failure <br> Report</h3>
          </div>
           <div class="col-md-4 text-center justify-content-center flex-column pt-4 rounded-3 mx-auto">
            <% if current_user.present? && current_user.lab_technician? || current_user.qa_lab_incharge?%>
              <a href="" disabled>
                <div class="py-0 d-flex justify-content-center align-items-center">
                  <%= image_tag("High_grade.png", alt: "Cement") %>
