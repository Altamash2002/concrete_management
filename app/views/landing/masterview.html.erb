              </a>
            <% else %>
              <%= link_to "/standard_deviation" do %>
                <div class="p-0">
                  <%= image_tag("Standard Deviation0.svg", alt: "Cement") %>
                </div>
              <% end %>
            <% end %>
            <h3 class="icon-heading">Standard <br> Deviation</h3>
          </div>
          <div class="col-md-4 text-center justify-content-center flex-column pt-4 rounded-3 mx-auto">
