<style>
  
  @media only screen and (min-width:321px) and (max-width:568px) {
    .s-label {
      text-align: center !important;
    }

    .s-label select {

      width: 100% !important;
    }

    .input {

      width: 100% !important;
    }

    .text-m {
      text-align: center !important;
      margin: 0 !important;
      padding: 0 !important;
    }
  }
  .previous-link{
    color: #565A8E;
  }
  .project{
    font-size: 16px; 
    font-weight: bold;
   }
</style>
<div style="background-color: #F5F5F5; overflow-x: hidden;">
  <%= render "/landing/filterbar" %>
  <div id="strength_testing_table"> 
    <div class="container">
      <div class="row d-flex">
        <div class="col-md-6">
        </div>
        <!-- <div class="col-md-6 text-right">
          <div class="">
            <%= link_to "/mastergraph", class: "btn btn-primary" do %>
              View all
            <% end %>
          </div>
        </div> -->
      </div>
    </div>
 </div> 
    <div class="col-md-5">
      <div class="row">
        <% if params[:building_id]%>
          <div class="col-md-2 previous-link">
            <%= link_to strength_graph_index_path(building_id: params[:building_id]), class: "dropdown-item child row align-items-center container-fluid m-0" do %>
              <span class="project">BUILDINGS ></span>
            <% end %>
          </div>
        <% end %>
        <% if params[:structure_id]%>
          <div class="col-md-2 previous-link">
            <%= link_to strength_graph_structure_path(building_id: params[:building_id]), class: "dropdown-item child row align-items-center container-fluid m-0" do %>
              <span class="project">STRUCTURES ></span>
            <% end %>
          </div>
        <% end %>
        <% if params[:grade_id]%>
          <div class="col-md-2 previous-link">
            <%= link_to strength_graph_grade_path(building_id: params[:building_id], structure_id: params[:structure_id]), class: "dropdown-item child row align-items-center container-fluid m-0" do %>
              <span class="project">&nbsp;&nbsp;GRADES ></span>
            <% end %>
          </div>
        <% end %>
      </div>
    </div>
    
  <%= render "/landing/concrete_graph" %>
</div>

 